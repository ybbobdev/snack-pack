{
   "private": true,
   "name": "snack-pack",
   "version": "1.0.0",
   "author": "Bob Menacho <bobmenacho@gmail.com>",
   "repository": "https://github.com/ybbobdev/snack-pack.git",
   "license": "MIT",
   "workspaces": [
      "packages/*"
   ],
   "scripts": {
      "source_secrets": "source secrets",
      "api": "yarn source_secrets && yarn workspace api dev",
      "site": "yarn source_secrets && yarn workspace site dev",
      "dev": "concurrently --kill-others-on-fail \"yarn site\"  \"yarn api\""
   },
   "dependencies": {
      "concurrently": "^6.1.0"
   }
}
